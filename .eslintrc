{
  "globals": {
    "jQuery": true,
    "$": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "import"
  ],
  "extends": [
    "airbnb-base"
  ],
  "rules": {
    "no-console": "off",
    "max-len": "off",
    "no-plusplus": "off",
    "no-underscore-dangle": "off",
    "func-names": [
      "warn",
      "as-needed"
    ],
    "quote-props": [
      "warn",
      "as-needed",
      {
        "keywords": true,
        "unnecessary": false
      }
    ]
  }
}
